{
    "manifest_version": 2,

    "name": "Covid19 Vaccine",
    "description": "",
    "version": "1.0",

    "browser_action":
    {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://code.jquery.com; object-src 'self'; connect-src 'self' 'unsafe-eval' https://cdn-api.co-vin.in",
    "permissions": ["notifications"],
    "background":
    {
        "scripts": ["background.js"],
        "persistent": false
    }
}